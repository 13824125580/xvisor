
/ {
	#address-cells = <1>;
	#size-cells = <1>;
	model = "imx25";

	chosen { };
	aliases { };

	cpus {
		#address-cells = <1>;
		#size-cells = <0>;

		cpu@0 {
			device_type = "cpu";
			model = "ARM926";
			reg = <0>;
		};
	};

	memory { 
		device_type = "memory"; 
		reg = <0x80000000 0x10000000>; /* 256 MB */
	};

	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "simple-bus";

		epit1 {
			device_type = "timer";
			compatible = "freescale,epit-timer";
			reg = <0x53f94000 0x4000>;
			clock-frequency = <38999040>;
			timer_num = <0>;
			interrupts = <28>;
		};

		epit2 {
			device_type = "timer";
			compatible = "freescale,epit-timer";
			reg = <0x53f98000 0x4000>;
			clock-frequency = <38999040>;
			timer_num = <1>;
			interrupts = <27>;
		};

		gpt1 {
			device_type = "timer";
			compatible = "freescale,gpt-timer";
			reg = <0x53f90000 0x4000>;
			clock-frequency = <1000000>;
			timer_num = <2>;
			interrupts = <54>;
		};

		gpt2 {
			device_type = "timer";
			compatible = "freescale,gpt-timer";
			reg = <0x53f8c000 0x4000>;
			clock-frequency = <1000000>;
			timer_num = <3>;
			interrupts = <53>;
		};

		gpt3 {
			device_type = "timer";
			compatible = "freescale,gpt-timer";
			reg = <0x53f88000 0x4000>;
			clock-frequency = <1000000>;
			timer_num = <4>;
			interrupts = <29>;
		};

		gpt4 {
			device_type = "timer";
			compatible = "freescale,gpt-timer";
			reg = <0x53f84000 0x4000>;
			clock-frequency = <1000000>;
			timer_num = <5>;
			interrupts = <1>;
		};

		avic {
			device_type = "pic";
			compatible = "freescale,avic";
			reg = <0x68000000 0x1000>;
		};

		SERIAL1: uart1 {
			compatible = "freescale,imx-uart";
			reg = <0x43f90000 0x4000>;
			clock-frequency = <3000000>;
			interrupts = <45>;
		};

		SERIAL2: uart2 {
			compatible = "freescale,imx-uart";
			reg = <0x43f94000 0x4000>;
			clock-frequency = <3000000>;
			interrupts = <32>;
		};

		SERIAL3: uart3 {
			compatible = "freescale,imx-uart";
			reg = <0x5000c000 0x4000>;
			clock-frequency = <3000000>;
			interrupts = <18>;
		};

		SERIAL4: uart4 {
			compatible = "freescale,imx-uart";
			reg = <0x50008000 0x4000>;
			clock-frequency = <3000000>;
			interrupts = <5>;
		};
	};
};

